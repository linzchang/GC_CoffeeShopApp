
@{
    ViewBag.Title = "Users";
}

<h2>Users</h2>

<table class="table">

    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Address</th>
        <th>Phone Number</th>
    </tr>
    @foreach (var user in ViewBag.UserList)
    {
        <tr>
            <td>@user.FirstName</td>
            <td>@user.LastName</td>
            <td>@user.Address</td>
            <td>@user.PhoneNumber</td>
        </tr>
    }

</table>
